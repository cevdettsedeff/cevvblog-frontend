# Vite Environment Variables
# VITE_ prefix zorunlu - sadece bu prefix ile başlayanlar client'a gönderilir

# API Configuration - Backend portunu kontrol edin
VITE_API_BASE_URL=http://localhost:3001/api
VITE_APP_NAME=CevvBlog
VITE_APP_VERSION=1.0.0

# Development
VITE_DEV_MODE=true
VITE_ENABLE_DEVTOOLS=true

# Production (bu değerler production'da override edilecek)
VITE_PROD_API_URL=https://your-production-api.com/api